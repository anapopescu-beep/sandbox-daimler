testcase SW_IT_MMG_CheckCycleEnable_04()
{
  //***********************************Test report header*********************************
  testReportAddMiscInfoBlock("AUTOLIV ELECTRONICS document");
  TestReportAddMiscInfo("Nightly test", "No");
  TestReportAddMiscInfo("Test type", "Requirement-Based test");
  TestReportAddMiscInfo("Regression Test", "No");
  TestReportAddMiscInfo("Fault injection method used", "No");
  TestReportAddMiscInfo("Resource usage evaluation", "No");
  TestReportAddMiscInfo("Priority", "No");
  TestReportAddMiscInfo("Safety relevant", "No");
  
  AddRequirementInfo("Requirement under test", "ARCH_SW_MMG_0315");
  AddRequirementInfo("ARCH_SW_MMG_0315", "If Step order is not a valid one (not 0x00: PWM order ; 0x01: Current order ; 0x02: Velocity Control order type; 0x03: Velocity Control + backup PWM order type; 0x04: Voltage order type; 0x05: Velocity Control + backup Current order type; 0x06: Velocity Control + backup Voltage order type ) then step is invalid.");
  //***************************************************************************************

  testCaseComment("Preconditions");
  testStep("STEP", "Bench Initialization");
  RunPreTs();
  
  
    testStep("STEP", "Set a breakpoint in function MMG_Init() at line 'mmg_CheckCycleEnable();'.");
    debugger_setRelativeFunctionBpAtLine("MMG_Init", 28);
  
    testStep("STEP", "Check if CPU stopped in function MMG_Init() at line 'mmg_CheckCycleEnable();'.");
    debugger_WaitUntilBPisReached(1000);  

    testStep("STEP", "Step in function mmg_CheckCycleEnable().");
    debugger_stepIn(); 
  
  
    testStep("STEP", "Set a breakpoint in function mmg_CheckIfCycleStepIsvalid() at line 'else if ( KU8_SEVEN == (NVP_BLOCK_ID_STEP_0_RamBlockData.NVP_au8StepsDefinitions[u16Step0Index0 + KU8_THREE] & MMG_STEPS_ORDER_TYPE_MASK ))'.");
    debugger_setRelativeFunctionBpAtLine("mmg_CheckIfCycleStepIsvalid", 29);
    
    debugger_run();
    
    testStep("STEP", "Check if CPU stopped in function mmg_CheckIfCycleStepIsvalid() at line 'else if ( KU8_SEVEN == (NVP_BLOCK_ID_STEP_0_RamBlockData.NVP_au8StepsDefinitions[u16Step0Index0 + KU8_THREE] & MMG_STEPS_ORDER_TYPE_MASK ))'.");
    debugger_WaitUntilBPisReached(1000);  
  
    testStep("STEP", "Set variable NVP_BLOCK_ID_STEP_0_RamBlockData.NVP_au8StepsDefinitions[u16Step0Index0 + KU8_THREE] to KU8_SEVEN.");
    debugger_writeGlobalVariable("uint8", "NVP_BLOCK_ID_STEP_0_RamBlockData,,app.elf", 3, 0x07);
  
  
    debugger_deleteAllBP();
  
  
    testStep("STEP", "Set a breakpoint in function mmg_CheckIfCycleStepIsvalid() at line 'b8Ret = B_FALSE;'.");
    debugger_setRelativeFunctionBpAtLine("mmg_CheckIfCycleStepIsvalid", 31);
    
    debugger_run();
    
    testCaseComment("Action: ARCH_SW_MMG_0315");
    testStep("STEP", "Check if CPU stopped in function mmg_CheckIfCycleStepIsvalid() at line 'b8Ret = B_FALSE;'.");
    debugger_WaitUntilBPisReached(1000);  
  
  
    debugger_run();
  
  
    testCaseComment("Action: ARCH_SW_MMG_0315");
    testStep("STEP", "Check if function mmg_CheckIfCycleStepIsvalid() returns B_FALSE. --> B_FALSE");
    result = debugger_readGlobalVariable("uint8", "Rte_MMG_psrEnableCycle0_b8CycleValidity,,app.elf", 0);
    EvalManStepEqual("Expected test result", result, 0x55);
  
    
  testCaseComment("Postcondition");
  testStep("STEP", "Return ECU to default values");
  RunPostTs();
}


