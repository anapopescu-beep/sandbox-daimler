                <CLIENT-SERVER-INTERFACE UUID="a995fcfb-dc4c-4749-8fda-ebb8ed6c2575">
                  <SHORT-NAME>SecOC_FreshnessManagement</SHORT-NAME>
                  <IS-SERVICE>true</IS-SERVICE>
                  <SERVICE-KIND>VENDOR-SPECIFIC</SERVICE-KIND>
                  <OPERATIONS>
                    <CLIENT-SERVER-OPERATION UUID="931f9946-af4b-4b2c-b9d9-137fa7e395bc">
                      <SHORT-NAME>GetRxFreshness</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This interface is used by the SecOC to obtain the current freshness value. This operation provides also a part of the Authentic-PDU data if configured.</L-2>
                      </DESC>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE UUID="4471355b-d73b-4a90-b62f-cb0cb2554fe4">
                          <SHORT-NAME>freshnessValueId</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">Identifier of the freshness</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint16</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="42dbabe4-1359-4bbb-ba16-8a0fefca533e">
                          <SHORT-NAME>truncatedFreshnessValue</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">The truncated freshness value from the received Secured-IPDU</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MICROSAR/SecOC_swc/DataTypes/freshnessValueType</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="68dd1484-231e-4aa2-8d8a-dc8557b4d2bb">
                          <SHORT-NAME>truncatedFreshnessValueLength</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">Length in bits of the truncated freshness value</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint32</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="f06e1632-5cd3-4208-89b3-a46a52b06e80">
                          <SHORT-NAME>authVerifyAttempts</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">The number of authentication verify attempts for the current PDU</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint16</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="e32d296a-289a-424b-b818-3f6dc6d69793">
                          <SHORT-NAME>freshnessValue</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">The freshness value for this PDU</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MICROSAR/SecOC_swc/DataTypes/freshnessValueType</TYPE-TREF>
                          <DIRECTION>OUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="6b095588-6818-4668-a2e4-cc6d081246d0">
                          <SHORT-NAME>freshnessValueLength</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">The freshness value length in bits.</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MICROSAR/SecOC_swc/DataTypes/freshnessValueLengthType</TYPE-TREF>
                          <DIRECTION>INOUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
                      <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/SecOC_FreshnessManagement/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/SecOC_FreshnessManagement/E_NOT_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/SecOC_FreshnessManagement/E_BUSY</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
                    </CLIENT-SERVER-OPERATION>
                  </OPERATIONS>
                  <POSSIBLE-ERRORS>
                    <APPLICATION-ERROR UUID="37e38b3b-fe44-4bf4-9345-c1e7f3e8a934">
                      <SHORT-NAME>E_OK</SHORT-NAME>
                      <ERROR-CODE>0</ERROR-CODE>
                    </APPLICATION-ERROR>
                    <APPLICATION-ERROR UUID="ceba72c8-fd70-4c54-84f4-3b583b43efbe">
                      <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                      <ERROR-CODE>1</ERROR-CODE>
                    </APPLICATION-ERROR>
                    <APPLICATION-ERROR UUID="19448079-1704-4a27-bb1e-b10200a1ce5b">
                      <SHORT-NAME>E_BUSY</SHORT-NAME>
                      <ERROR-CODE>2</ERROR-CODE>
                    </APPLICATION-ERROR>
                  </POSSIBLE-ERRORS>
                </CLIENT-SERVER-INTERFACE>
                <CLIENT-SERVER-INTERFACE UUID="c9f33530-959b-4bba-b8bf-47881c8b1f53">
                  <SHORT-NAME>SecOC_VerifyStatusConfiguration</SHORT-NAME>
                  <IS-SERVICE>true</IS-SERVICE>
                  <SERVICE-KIND>VENDOR-SPECIFIC</SERVICE-KIND>
                  <OPERATIONS>
                    <CLIENT-SERVER-OPERATION UUID="814a77d3-3f72-4025-918b-dca46e9fda11">
                      <SHORT-NAME>VerifyStatusOverride</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL"/>
                      </DESC>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE UUID="48d218e6-74ae-4f99-97b4-11c23ae412df">
                          <SHORT-NAME>freshnessValueID</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">ID of the Freshness Value which when used to authenticate data, results in SecOC_VerifyStatus equal to OverrideStatus independent of the actual authentication status.</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint16</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="255b6008-b140-4c95-8f64-a9ea8498ec32">
                          <SHORT-NAME>overrideStatus</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">Defines whether verification is executed and whether the I-PDU is passed on, and for how long the override is active.</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE UUID="29679db3-18cd-4ac7-bfaf-5ae2f7547d11">
                          <SHORT-NAME>numberOfMessagesToOverride</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL">Number of sequential VerifyStatus to override when using a specific counter for authentication verification.</L-2>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
                      <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/SecOC_VerifyStatusConfiguration/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/SecOC_VerifyStatusConfiguration/E_NOT_OK</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
                    </CLIENT-SERVER-OPERATION>
                  </OPERATIONS>
                  <POSSIBLE-ERRORS>
                    <APPLICATION-ERROR UUID="3e7b6bd4-49a0-4697-a37f-4fd417f76d95">
                      <SHORT-NAME>E_OK</SHORT-NAME>
                      <ERROR-CODE>0</ERROR-CODE>
                    </APPLICATION-ERROR>
                    <APPLICATION-ERROR UUID="86b62cb6-4622-44a4-b9e1-b04b30b8fc54">
                      <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                      <ERROR-CODE>1</ERROR-CODE>
                    </APPLICATION-ERROR>
                  </POSSIBLE-ERRORS>
                </CLIENT-SERVER-INTERFACE>
                <CLIENT-SERVER-INTERFACE UUID="1f7d0d0d-5f93-4ea9-b720-06a8dc395f3f">
                  <SHORT-NAME>VerificationStatusIndication</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">This service realizes a notification service that is used to propagate the status of an authentication attempt from the SecOC module to an SW-C through RTE.</L-2>
                  </DESC>
                  <IS-SERVICE>true</IS-SERVICE>
                  <SERVICE-KIND>VENDOR-SPECIFIC</SERVICE-KIND>
                  <OPERATIONS>
                    <CLIENT-SERVER-OPERATION UUID="59bd532c-d431-4700-9407-97f7ece14c4f">
                      <SHORT-NAME>VerifyStatus</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This service provides the ability to inform the application about the result of the verification attempt of a received PDU by the SecOC module.</L-2>
                      </DESC>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE UUID="2ed44a36-99b6-43e2-a5fb-96bd8777f480">
                          <SHORT-NAME>verificationStatus</SHORT-NAME>
                          <DESC>
                            <L-2 L="FOR-ALL"/>
                          </DESC>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MICROSAR/SecOC_swc/DataTypes/SecOC_VerificationStatusType</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
                      <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/VerificationStatusIndication/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/MICROSAR/SecOC_swc/Interfaces/VerificationStatusIndication/E_NOT_OK</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
                    </CLIENT-SERVER-OPERATION>
